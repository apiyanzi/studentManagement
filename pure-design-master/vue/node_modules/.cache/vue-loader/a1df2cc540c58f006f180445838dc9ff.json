el-table-column>\n      <el-table-column prop=\"name\" label=\"文章标题\"></el-table-column>\n      <el-table-column prop=\"user\" label=\"发布人\"></el-table-column>\n      <el-table-column prop=\"time\" label=\"发布时间\"></el-table-column>\n    </el-table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'TimeSearch',\n  data() {\n    return {\n      datetime: null,\n      tableData: [],\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      console.log(this.datetime);\n      // if (!this.datetime) {\n      //   return\n      // }\n      let start = '';\n      let end = '';\n      if (this.datetime && this.datetime.length >= 2) {\n        start = this.datetime[0];\n        end = this.datetime[1];\n      }\n      // 根据筛选条件发请求\n      this.request\n        .get('/article', {\n          params: {\n            start: start,\n            end: end,\n          },\n        })\n        .then(res => {\n          this.tableData = res.data;\n        });\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]}]}